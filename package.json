{
  "name": "cruiseaide",
  "version": "1.0.0",
  "description": "Web Project based on Nuxt.js, Vue.js, Vuetify, Express.js, Axios, Firebase",
  "author": "Zubair Ijaz",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
